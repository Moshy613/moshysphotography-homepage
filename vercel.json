{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "functions": {
    "functions/src/index.ts": {
      "runtime": "@vercel/node@18"
    }
  },
  "env": {
    "OPENAI_API_KEY": "@openai_api_key"
  }
}